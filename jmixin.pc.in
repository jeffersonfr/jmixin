prefix="@CMAKE_INSTALL_PREFIX@"
exec_prefix="${prefix}"
libdir="${prefix}/lib"
includedir="${prefix}/include"

Name: jMixin
Description: A library mixin some standard functionalities
URL: https://github.com/me/jeffersonfr/jmixin
Version: 0.0.1
Requires: @PKGCONF_REQ_PUB@
Requires.private: @PKGCONF_REQ_PRIV@
Cflags: -I"${includedir}" 
Libs: -L"${libdir}" -ljmixin
Libs.private: -L"${libdir}" -ljmixin
